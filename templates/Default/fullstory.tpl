<div class="speedbar nowrap icon-left">
    <span class="far fa-home"></span>
    <span itemscope="" itemtype="https://schema.org/BreadcrumbList">
        <span itemprop="itemListElement" itemscope="" itemtype="https://schema.org/ListItem">
            <meta itemprop="position" content="1" /><a href="/" itemprop="item"><span itemprop="name">Bosh sahifa</span></a>
        </span>
        »
        <span itemprop="itemListElement" itemscope="" itemtype="https://schema.org/ListItem">
            {link-category}
        </span>
        » {title}
    </span>
</div>
<article class="article">
    <div class="fmain">
        <div class="fheader ignore-select">
            <h1>{title}</h1>
            <div class="fmeta fx-row fx-middle fx-start icon-left">
                <div class="fmeta-item"><span class="far fa-calendar-alt"></span>{date}</div>
                <div class="fmeta-item fx-1 mhid1"><span class="far fa-eye"></span>{views}</div>
            </div>
        </div>
        <div class="ftext full-text video-box clearfix">
            {full-story}
        </div>
        <div class="fbtm fx-row fx-middle ignore-select">
            <div class="ferror fx-row fx-middle fx-1">
                <div class="ferror-btns fx-row">
                    <div>Ctrl</div>
                    <div>Enter</div>
                </div>
                <div class="ferror-text fx-1">
                    <div>Xatoli<span>g</span>ni aqniqladingizmi?</div>
                    Matnni belgilang va <span>Ctrl+Enter</span> tugmalarini bosing.
                </div>
            </div>
        </div>
    </div>
    <!-- END FMAIN -->
    <div class="sect sect-border ignore-select">
        <div class="sect-title">
            <span><span class="far fa-link"></span>Hamda bularni ko'ring</span>
        </div>
        <div class="sect-content">
            {related-news}
        </div>
    </div>

    <div class="fcomms hidden" id="full-comms"><!--dleaddcomments--><!--dlecomments--><!--dlenavigationcomments--></div>
</article>
