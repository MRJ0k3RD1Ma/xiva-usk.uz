<article class="first cycle-slide cycle-slide-active" style="position: absolute; top: 0px; left: 0px; z-index: 99; opacity: 1; display: block; visibility: visible;height: 460px;overflow: hidden;">
    <img style="object-fit: cover;" src="{image-1}" class="attachment-start-magazine-featured size-start-magazine-featured wp-post-image" style="width: 100%;height: 460px;overflow: hidden;"/>
    <div class="slide-caption">
        <span class="posts-slider-category">{link-category}</span>

        <h3><a href="{full-link}">{title}</a></h3>
        <div class="post-slider-meta entry-meta">
            <span class="posted-on">{date}</span>
        </div>
    </div>
    <!-- .slide-caption -->
</article>
